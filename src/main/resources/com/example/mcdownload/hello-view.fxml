<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.mcdownload.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
    <Button text="Grill" onAction="#grillButt"/>
    <Button text="Friteza" onAction="#fritezaButt"/>
    <Button text="Iniciator" onAction="#iniciatorButt"/>
    <Button text="Assembler" onAction="#assemblerButt"/>
    <Button text="Finisher" onAction="#finisherButt"/>
</VBox>
